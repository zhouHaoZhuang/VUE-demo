    "jspdf": "^2.5.1",
    "kscreenshot": "^1.2.3",
    "swiper": "^6.8.4",
    "vue": "^2.6.14",
    "vue-awesome-swiper": "^4.1.1",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2",
    "weixin-js-sdk": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
